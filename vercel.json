{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { 
      "source": "/",
      "destination": "/index.html"
    },
    { 
      "source": "/thank_you",
      "destination": "/thank_you.html"
    },
    { 
      "source": "/whitepaper", 
      "destination": "/whitepaper.html" 
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
